{
    "name": "dolphindb-web",
    "engines": {
        "node": ">=17.4.0"
    },
    "type": "module",
    "scripts": {
        "dev": "tsc --project ./tsconfig.lib.json && node ./dev.js",
        "server": "tsc --project ./tsconfig.lib.json && cross-env NODE_EXTRA_CA_CERTS=./server/cert/dolphindb.crt node ./server/index.js",
        "typecheck": "tsc --noEmit",
        "build": "tsc --project ./tsconfig.lib.json && node ./build.js",
        "build.cloud": "tsc --project ./tsconfig.lib.json && node ./build.js cloud",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\""
    },
    "author": "Hongfei Shen <hongfei.shen@dolphindb.com> (https://dolphindb.net/hongfeishen)",
    "homepage": "https://dolphindb.net/dolphindb/dolphindb_web",
    "repository": {
        "type": "git",
        "url": "https://dolphindb.net/dolphindb/dolphindb_web.git"
    },
    "devDependencies": {
        "@svgr/webpack": "^6.2.1",
        "@types/koa": "^2.13.4",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "@types/sass-loader": "^8.0.3",
        "css-loader": "^6.6.0",
        "react": "^17.0.2",
        "sass": "^1.49.9",
        "sass-loader": "^12.6.0",
        "source-map-loader": "^3.0.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.6.2",
        "webpack": "^5.69.1"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "antd": "^4.18.9",
        "cross-env": "^7.0.3",
        "dayjs": "^1.10.8",
        "final-form": "^4.20.6",
        "prismjs": "^1.27.0",
        "react-dom": "^17.0.2",
        "react-object-model": "^0.0.11",
        "react-simple-code-editor": "^0.11.0",
        "tslib": "^2.3.1",
        "xshell": "^0.0.33",
        "xterm": "^4.18.0",
        "xterm-addon-fit": "^0.5.0"
    }
}
