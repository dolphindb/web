    "license": "MIT",
    "homepage": "https://dolphindb.net/dolphindb/dolphindb_web",
    "repository": {
        "type": "git",
        "url": "https://dolphindb.net/dolphindb/dolphindb_web"
    },
    "bugs": {
        "url": "https://dolphindb.net/dolphindb/dolphindb_web/issues"
    },
    "keywords": [
        "DolphinDB",
        "console",
        "database",
        "web"
    ],
    "devDependencies": {
        "@svgr/webpack": "^6.2.1",
        "@types/koa": "^2.13.4",
        "@types/node": "^17.0.17",
