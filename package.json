    "name": "dolphindb-web",
    "type": "module",
    "engines": {
        "node": ">=21.0.0"
    },
    "scripts": {
        "install.ci": "pnpm ci --include=dev",
        "dev": "tsc --project ./tsconfig.node.json && node ./dev.js",
        "dev:watch": "tsc --project ./tsconfig.node.json && node ./dev.js --watch",
        "typecheck": "tsc --noEmit",
        "build": "tsc --project ./tsconfig.node.json && node ./build.js",
        "build.cloud": "tsc --project ./tsconfig.node.json && node ./build.js cloud",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\"",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "fix": "eslint --fix \"**/*.{ts,tsx}\""
    },
