{
    "name": "dolphindb-web",
    "engines": {
        "node": ">=17.4.0"
    },
    "type": "module",
    "scripts": {
        "install.ci": "npm ci --include=dev --force",
        "dev": "tsc --project ./tsconfig.lib.json && node ./dev.js",
        "server": "tsc --project ./tsconfig.lib.json && cross-env NODE_EXTRA_CA_CERTS=./server/cert/dolphindb.crt node ./server/index.js",
        "typecheck": "tsc --noEmit",
        "build": "tsc --project ./tsconfig.lib.json && node ./build.js",
        "build.cloud": "tsc --project ./tsconfig.lib.json && node ./build.js cloud",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\""
    },
    "author": "Hongfei Shen <hongfei.shen@dolphindb.com> (https://dolphindb.net/hongfeishen)",
    "homepage": "https://dolphindb.net/dolphindb/dolphindb_web",
    "repository": {
        "type": "git",
        "url": "https://dolphindb.net/dolphindb/dolphindb_web.git"
    },
    "devDependencies": {
        "@svgr/webpack": "^6.2.1",
        "@types/koa": "^2.13.4",
        "@types/koa-compress": "^4.0.3",
        "@types/node": "^17.0.31",
        "@types/react": "^18.0.8",
        "@types/react-dom": "^18.0.3",
        "@types/sass-loader": "^8.0.3",
        "css-loader": "^6.7.1",
        "react": "^18.1.0",
        "sass": "^1.51.0",
        "sass-loader": "^12.6.0",
        "source-map-loader": "^3.0.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.3.0",
        "typescript": "^4.6.4",
        "webpack": "^5.72.0"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@monaco-editor/react": "^4.4.4",
        "antd": "^4.20.2",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.1",
        "dolphindb": "^2.0.518",
        "final-form": "^4.20.7",
        "koa-compress": "^5.1.0",
        "prismjs": "^1.28.0",
        "react-dom": "^18.1.0",
        "react-object-model": "^0.0.15",
        "tslib": "^2.4.0",
        "upath": "^2.0.1",
        "xshell": "^0.0.49",
        "xterm": "^4.18.0",
        "xterm-addon-fit": "^0.5.0"
    }
}
