{
    "name": "dolphindb-web",
    "engines": {
        "node": ">=18.4.0"
    },
    "type": "module",
    "scripts": {
        "install.ci": "npm ci --include=dev --force",
        "dev": "tsc --project ./tsconfig.node.json && node ./dev.js",
        "server": "tsc --project ./tsconfig.node.json && cross-env NODE_EXTRA_CA_CERTS=./server/cert/dolphindb.crt node ./server/index.js",
        "typecheck": "tsc --noEmit",
        "build": "tsc --project ./tsconfig.node.json && node ./build.js",
        "build.cloud": "tsc --project ./tsconfig.node.json && node ./build.js cloud",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\""
    },
    "author": "Hongfei Shen <hongfei.shen@dolphindb.com> (https://dolphindb.net/hongfeishen)",
    "homepage": "https://github.com/dolphindb/web",
    "repository": {
        "type": "git",
        "url": "https://github.com/dolphindb/web.git"
    },
    "overrides": {
        "rc-trigger": "^5.3.1"
    },
    "devDependencies": {
        "@svgr/webpack": "^6.2.1",
        "@types/koa": "^2.13.4",
        "@types/koa-compress": "^4.0.3",
        "@types/lodash": "^4.14.182",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/sass-loader": "^8.0.3",
        "css-loader": "^6.7.1",
        "react": "^18.2.0",
        "sass": "^1.53.0",
        "sass-loader": "^13.0.2",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.3.1",
        "typescript": "^4.7.4",
        "webpack": "^5.73.0"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@ant-design/plots": "^1.0.9",
        "@monaco-editor/react": "^4.4.5",
        "antd": "^4.21.4",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.3",
        "dolphindb": "^2.0.602",
        "final-form": "^4.20.7",
        "koa-compress": "^5.1.0",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.0",
        "re-resizable": "^6.9.9",
        "react-dom": "^18.2.0",
        "react-object-model": "^0.0.19",
        "tslib": "^2.4.0",
        "upath": "^2.0.1",
        "xshell": "^0.0.58",
        "xterm": "^4.18.0",
        "xterm-addon-fit": "^0.5.0"
    }
}
