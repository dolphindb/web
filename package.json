{
    "name": "dolphindb-web",
    "engines": {
        "node": ">=18.8.0"
    },
    "type": "module",
    "scripts": {
        "install.ci": "npm ci --include=dev --force",
        "dev": "tsc --project ./tsconfig.node.json && node ./dev.js",
        "server": "tsc --project ./tsconfig.node.json && cross-env NODE_EXTRA_CA_CERTS=./server/cert/dolphindb.crt node ./server/index.js",
        "typecheck": "tsc --noEmit",
        "build": "tsc --project ./tsconfig.node.json && node ./build.js",
        "build.cloud": "tsc --project ./tsconfig.node.json && node ./build.js cloud",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\""
    },
    "author": "Hongfei Shen <hongfei.shen@dolphindb.com> (https://dolphindb.net/hongfeishen)",
    "homepage": "https://github.com/dolphindb/web",
    "repository": {
        "type": "git",
        "url": "https://github.com/dolphindb/web.git"
    },
    "overrides": {
        "rc-trigger": "^5.3.1"
    },
    "devDependencies": {
        "@svgr/webpack": "^6.5.1",
        "@types/koa": "^2.13.5",
        "@types/koa-compress": "^4.0.3",
        "@types/lodash": "^4.14.190",
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "@types/sass-loader": "^8.0.3",
        "css-loader": "^6.7.2",
        "react": "^18.2.0",
        "sass": "^1.56.1",
        "sass-loader": "^13.2.0",
        "source-map-loader": "^4.0.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.4.1",
        "typescript": "^4.9.3",
        "webpack": "^5.75.0"
    },
    "dependencies": {
        "@ant-design/icons": "^4.8.0",
        "@ant-design/plots": "^1.2.2",
        "@ant-design/pro-layout": "^7.2.6",
        "@monaco-editor/react": "^4.4.6",
        "antd": "^5.0.2",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.6",
        "dolphindb": "^2.0.801",
        "final-form": "^4.20.7",
        "koa-compress": "^5.1.0",
        "lodash": "^4.17.21",
        "nanoid": "^4.0.0",
        "re-resizable": "^6.9.9",
        "react-dom": "^18.2.0",
        "react-object-model": "^0.0.23",
        "tslib": "^2.4.1",
        "upath": "^2.0.1",
        "vscode-oniguruma": "^1.7.0",
        "vscode-textmate": "^7.0.4",
        "xshell": "^0.0.64",
        "xterm": "^5.0.0",
        "xterm-addon-fit": "^0.6.0",
        "xterm-addon-web-links": "^0.7.0",
        "xterm-addon-webgl": "^0.13.0"
    }
}
