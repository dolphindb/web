{
    "name": "dolphindb-web",
    "description": "DolphinDB Web",
    "version": "0.0.1",
    "type": "module",
    "main": "./index.tsx",
    "scripts": {
        "compile": "tsc --project ./tsconfig.lib.json --watch",
        "server": "node ./server.js",
        "typecheck": "tsc --noEmit",
        "build": "tsc --project ./tsconfig.lib.json && node ./build.js",
        "build.cloud": "tsc --project ./tsconfig.lib.json && node ./build.js cloud",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\""
    },
    "engines": {
        "node": ">=17.3.1"
    },
    "author": {
        "name": "Hongfei Shen",
