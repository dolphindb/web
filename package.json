        "@types/react-dom": "^17.0.11",
        "@types/sass-loader": "^8.0.3",
        "css-loader": "^6.6.0",
        "react": "^17.0.2",
        "sass": "^1.49.7",
        "sass-loader": "^12.4.0",
        "source-map-loader": "^3.0.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.5",
        "webpack": "^5.68.0"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "antd": "^4.18.6",
        "dayjs": "^1.10.7",
        "final-form": "^4.20.6",
        "prismjs": "^1.26.0",
        "react-dom": "^17.0.2",
        "react-object-model": "^0.0.10",
